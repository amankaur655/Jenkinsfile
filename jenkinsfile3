pipeline {
    agent any
    stages{
        stage('check current directory'){
            steps{ docker { image 'node:20.10.0-alpine3.19' }}
                sh 'pwd'
               sh 'id'
               sh "mkdir \$(date '+%Y%m%d_%H%M%S')"
                sh 'npm --version'
            }
        }
    stage('python script'){
            agent {docker { image 'python:3.10-slim' }}
            steps{
              sh 'python3 --version'  
            }
        }
    }
